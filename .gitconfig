[core]
	editor = nvim
[init]
	defaultBranch = main
[push]
	autoSetupRemote = true
[pull]
	ff = only
[alias]
	af = !git-utils add-fuzzy
	amend = commit --amend
	co = checkout
	cp = cherry-pick
	fixup = commit --fixup
	graph = !git-utils graph
	pf = push --force-with-lease
	ri = rebase --interactive --autosquash
	root = !git-utils root
	sw = switch
	sf = !git-utils switch-fuzzy
	sm = !git-utils switch-to-main
	um = !git-utils update-main
	unstage = restore --staged

[include]
	path = ~/.config/git/local.ini
