[core]
	editor = nvim
[init]
	defaultBranch = main
[push]
	autoSetupRemote = true
[pull]
	ff = only
[alias]
	graph = log --graph --oneline --all
	unstage = restore --staged
	amend = commit --amend
	fixup = commit --fixup
	co = checkout
	sw = switch
	cp = cherry-pick
	ri = rebase --interactive --autosquash

[include]
	path = ~/.config/git/local.ini
